{
  "expo": {
    "name": "Todo List",
    "slug": "todoList",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/newIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/newIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.yourappname",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.mansoor.ameen.todo",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/newIcon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/newIcon.png"
    }
  }
}
